// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id String @id
  name String
  email String @unique
  password String
  role String
}

model position {
  id String @id
  name String @unique
}

model history {
  id String @id @default(uuid())
  stack String
}
